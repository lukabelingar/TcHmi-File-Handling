{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "ES2020",
    "skipLibCheck": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019",
      "ES2020.BigInt"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "exclude": [
    "$(Output).Path/"
  ],
  "include": [
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiFileExplorer/TcHmiFileExplorer.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiCombobox/TcHmiCombobox.d.ts"
  ],
  "files": [
    "Scripts/UploadFromClientToHmi.ts",
    "Scripts/DownloadFromHmiToClient.ts",
    "Scripts/SaveFromHmiToTarget.ts",
    "Scripts/LoadFromTargetToHmi.ts",
    "Scripts/DeleteFileFromTarget.ts",
    "Scripts/BuildPath.ts",
    "Scripts/ListFilesFromTarget.ts"
  ]
}